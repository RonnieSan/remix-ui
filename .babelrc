{
	"presets" : [
		[
			"@babel/env",
			{
				"targets" : {
					"browsers" : [
						"Chrome >= 52",
						"FireFox >= 44",
						"Safari >= 7",
						"Explorer 11",
						"last 4 Edge versions"
					]
				},
				"useBuiltIns" : "usage",
				"corejs" : 3
			}
		]
	],
	"plugins": [
		"@babel/plugin-transform-runtime",
		"lodash"
	]
}